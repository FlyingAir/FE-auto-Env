<div class="statusbar-overlay"></div>
<div class="panel-overlay"></div>
<div class="views">
    <div class="view view-main">
        <div class="pages navbar-fixed">
            <div data-page="address_add" class="page page-on-center mypage_member-drop_page">
                <% include ./_parts/navbar %>

                <div class="page-content">

                    <section class="mypage_member-drop_content">

                        <div class="password_wrap">
                            <h5 class="wrap_title">title</h5>
                            <input type="password" placeholder="password" />
                        </div>

                        <div class="reason_wrap">
                            <h5 class="wrap_title">title</h5>
                            <ul>
                                <%for(var i = 0 ; i < 6 ; i++){%>
                                <li class="li_with_checkbox">
                                    <label class="label-checkbox item-content agree_check">
                                        <input type="checkbox" name="name" value="value" checked="checked">
                                        <div class="item-media"><i class="icon icon-form-checkbox"></i>checkname</div>
                                    </label>
                                </li>                                
                                <%}%>
                                <li class="li_with_checkbox_and_textarea">
                                    <label class="label-checkbox item-content agree_check">
                                        <input type="checkbox" name="name" value="value" checked="checked">
                                        <div class="item-media"><i class="icon icon-form-checkbox"></i>checkname</div>
                                    </label>
                                    <textarea></textarea>
                                </li>                                
                            </ul>
                        </div>

                        <div class="bottom_wrap">
                            <h5 class="wrap_title">title</h5>
                            <div class="checkbox_wrap">
                                <label class="label-checkbox item-content agree_check">
                                    <input type="checkbox" name="name" value="value" checked="checked">
                                    <div class="item-media"><i class="icon icon-form-checkbox"></i>checkname</div>
                                </label>
                            </div>
                        </div>

                        <div class="bottom_btn">
                            click me !
                        </div>
                    </section>
                </div>

                <% include ./_parts/toolbar %>
            </div>
        </div>
    </div>
</div>




